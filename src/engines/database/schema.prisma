generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id        String    @id
  name      String
  email     String    @unique
  hash      String
  createdAt DateTime  @default(now())
  updateAt  DateTime  @default(now())
  deletedAt DateTime?

  @@map("users")
}
